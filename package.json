{
  "name": "pt3react",
  "version": "1.0.0",
  "description": "A boilerplate of react projects.",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node webpack-browsersync.js",
    "build": "npm run clean && webpack --config webpack-prod.config.js --colors",
    "test": "mocha --require ./test/_setup.js --compilers babel-core/register ./test/**/*test.js",
    "test:watch": "npm test -- -w"
  },
  "repository": {
    "type": "git"
  },
  "author": "greenishsea",
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.0",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.2.3",
    "html-webpack-plugin": "^2.17.0",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.2.8",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "jquery": "^3.1.1",
    "jwt-decode": "^2.1.0",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.2.1",
    "react-images": "^0.5.1",
    "react-modal": "^1.5.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-scroll": "^0.4.1",
    "react-social": "^1.6.2",
    "react-sortable-hoc": "^0.2.0",
    "redux": "^3.5.2",
    "redux-form": "^5.3.1",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  }
}
